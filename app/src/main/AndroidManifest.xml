<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sunny.sensorexhibit" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Gravity"></activity>
        <activity android:name=".Orientation"></activity>
        <activity android:name=".Pressure"></activity>
        <activity android:name=".Proximity"></activity>
        <activity android:name=".Rotation"></activity>
        <activity android:name=".Temperature"></activity>
        <activity android:name=".WelcomeActivity"></activity>
        <activity android:name=".Accelerometer"></activity>
        <activity android:name=".AmbientTemperature"></activity>
        <activity android:name=".Gyroscope"></activity>
        <activity android:name=".Light"></activity>
        <activity android:name=".LinearAccelerometer"></activity>
        <activity android:name=".ListViewActivity"></activity>
        <activity android:name=".Magnetic"></activity>
    </application>

</manifest>